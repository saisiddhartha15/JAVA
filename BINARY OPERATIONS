import java.util.*;

public class Main {
    public static void main(String[] args) {
      
     
      int a = 15;
      int b = 5;

      System.out.println(a+b);
      System.out.println(a-b);
      System.out.println(a*b);
      System.out.println(a/b);
      System.out.println(a%b);
      a = a+2;
      System.out.println(a);
      a = a-2;
      System.out.println(a);
      b = b*2;
      System.out.println(b);
      b = b/2;
      System.out.println(b);
      
  }
}
