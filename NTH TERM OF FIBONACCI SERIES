import java.util.Scanner;

public class Main{
  public static int fib(int N){
    if(N==0)
      return 0;
    else if(N==1)
      return 1;
    else
      return fib(N-1) + fib(N-2);
}
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    int N = sc.nextInt();
    System.out.print(fib(N));
  }
}
